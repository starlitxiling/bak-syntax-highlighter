{
  "name": "bak-syntax-highlighter",
  "displayName": "BAK Syntax Highlighter",
  "description": "Auto-detects .py.bak, .cpp.bak, and other backup files and applies the correct syntax highlighting in VS Code.",
  "version": "0.0.1",
  "publisher": "starlitxiling",
  "repository": {
    "type": "git",
    "url": "https://github.com/starlitxiling/bak-syntax-highlighter"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:plaintext",
    "onFileSystem:file",
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": []
  },
  "categories": [
    "Other",
    "Languages"
  ],
  "keywords": [
    "syntax",
    "highlight",
    "language mode",
    "bak",
    "backup",
    "auto detect",
    "python",
    "cpp",
    "file extension"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.50.0",
    "@types/node": "^14.14.6",
    "typescript": "^4.0.3",
    "eslint": "^7.10.0",
    "vscode-test": "^1.4.0"
  }
}